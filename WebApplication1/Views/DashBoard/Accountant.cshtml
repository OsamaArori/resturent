@model Tuple<IEnumerable<WebApplication1.Models.Customer>,IEnumerable<WebApplication1.Models.Product>>
@{
	Layout = "~/Views/Shared/UserDashboard.cshtml";
}
	<head>
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
	</head>



<div class="container">
	<div class="row">
	<div class="col-lg-6 col-md-6">
	<div class="card">
		<div class="card-header card-header-warning text-white bg-warning">
			<h4 class="card-title">Clients</h4>
			<p class="card-category"></p>
		</div>
		<div class="card-body table-responsive">
			<table class="table table-hover">
				<thead class="text-warning"> 
					<tr>
						<th>First Name</th>
						<th>Last Name</th>
					</tr>
				</thead>
				<tbody>
					@if(Model.Item1 != null)
						{
							foreach (var item in Model.Item1)
							{
								<tr>
									<td>@item.Fname</td>
									<td>@item.Lname</td>
									</tr>
								}
							}
							else
							{
								<h1>There is no data to found</h1>
								
							}
						</tbody>
			</table>
		</div>
		</div>
	</div>
	<div class="col-lg-6 col-md-6">
	<div class="card">
		<div class="card-header card-header-warning  text-white bg-danger">
			<h4 class="card-title">Products</h4>
			<p class="card-category"></p>
		</div>
		<div class="card-body table-responsive">
			<table class="table table-hover">
				<thead class="text-warning"> 
					<tr>
						<th>price</th>
						<th>Product Name</th>
					</tr>
				</thead>
				<tbody>
					@if(Model.Item2 != null)
						{
							foreach (var item in Model.Item2)
							{
								<tr>
									<td>@item.Price</td>
									<td>@item.Name</td>
								</tr>
							}
						}
						else
							{
								<h1>There is no data to found</h1>
								
							}
					</tbody>
			</table>
		</div>
	</div>
	
	</div>
	</div>
</div>